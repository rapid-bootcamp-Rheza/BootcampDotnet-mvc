@model KosViewModel
@{
    ViewData["Title"] = "Kos Add Data";
}

<div >
    <h1 >Form Add Penyewa Kos</h1>
    <form asp-action="Save" method="post">
        <div class="form-group">
            <label asp-for="Id">Customer Id</label>
            <input asp-for="Id" class="form-control">
        </div>

        <div class="form-group">
            <label asp-for="NamaPenyewa">Nama Penyewa</label>
            <input asp-for="NamaPenyewa" class="form-control">
        </div>

        <div class="form-group">
            <label asp-for="Gender">Gender</label>
            <input asp-for="Gender" class="form-control">
        </div>

        <div class="form-group">
            <label asp-for="Status">Status</label>
            <input asp-for="Status" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="HargaSewa">Harga Sewa</label>
            <input asp-for="HargaSewa" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="JenisKamar">Jenis Kamar</label>
            <input asp-for="JenisKamar" class="form-control">
        </div>
        <div class="form-group">
            <label asp-for="Waktu">Waktu Sewa (Bulan)</label>
            <input asp-for="Waktu" class="form-control">
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

    @section Scripts{
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
</div>
