@model CustomerViewModel
@{
    ViewData["Title"] = "Customer Details";
}

<div >
    <h2>Product Detail</h2>
    <table calss = "table tab-content">
        <tbody>
            <tr>
                <th>ID</th>
                <td>@Model.CustomerId</td>
            </tr>
            <tr>
                <th>Customer Name</th>
                <td>@Model.CustomerName</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>@Model.Gender</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>@Model.Address</td>
            </tr>
            <tr>
                <th>City</th>
                <td>@Model.City</td>
            </tr>
            <tr>
                <th>Province</th>
                <td>@Model.Province</td>
            </tr>
         
        </tbody>
    </table>
</div>
