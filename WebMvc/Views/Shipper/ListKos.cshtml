@model List<ShipperViewModel>
@{
    ViewData["Title"] = "Shipper List";
}

<div >
    <h2>Shipper List</h2>
    <table class="table table-responsive">
        <thead>
            <tr>
                <th>Shipper ID</th>
                <th>Shipper Name</th>
                <th>Jenis Produk</th>
                <th>Region</th>
                <th>Harga per Kilo</th>
                <th>Jumlah Cabang</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.NameShipper</td>
                    <td>@item.Production</td>
                    <td>@item.Region</td>
                    <td>@item.PriceShipper</td>
                    <td>@item.Branch</td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-sm btn-warning">Edit</a>
                        <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-sm btn-success">View</a>
                        <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-sm btn-danger">Delete</a>
                    </td>
                </tr>
               
            }
            
        </tbody>
        <a asp-action="Add" class="btn btn-sm btn-submit">Add</a>
      

    </table>
</div>
